{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": ["id", "ip", "uuid"],
  "properties": {
    "id": {
      "type": "number",
      "description": "Visitor ID."
    },
    "ip": {
      "type": "string",
      "description": "Visitor IP."
    },
    "uuid": {
      "type": "string",
      "description": "Visitor UUID."
    }
  }
}
